{
    "name": "fitgirl-scraper",
    "version": "1.0.0",
    "description": "Scrappr for FitGirl Repacks. This script scrapes the FitGirl Repacks website to gather information and download links (magnet, FuckingFast and DataNodes).",
    "main": "compare.js",
    "scripts": {
        "compare": "node --no-warnings compare.js",
        "check": "node --no-warnings compare.js --check",
        "count": "node --no-warnings compare.js --count-items",
        "find": "node --no-warnings find.js",
        "ddl": "node --no-warnings ddl.js",
        "date": "node --no-warnings fixDate.js",
        "fetch": "node --no-warnings fetchAll.js",
        "pages": "node --no-warnings fetchAll.js --all"
    },
    "keywords": [],
    "author": "Matsumurae",
    "license": "MIT",
    "dependencies": {
        "@vladmandic/pilogger": "^0.5.1",
        "async-lock": "^1.4.1",
        "dotenv": "^17.2.1",
        "puppeteer": "^24.15.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2"
    }
}
